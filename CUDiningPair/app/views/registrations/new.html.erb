<%= form_for :user, url: registrations_path do |f| %>
    <div class="field">
      <%= f.label :uni, "UNI" %><br>
      <%= f.text_field :uni %>
    </div>
    <div class="field">
      <%= f.label :password %><br>
      <%= f.password_field :password %>
    </div>
    <div class="field">
    <%= f.label :password_confirmation %><br>
    <%= f.password_field :password_confirmation %>
    </div>
    <div class="actions">
      <%= f.submit "Sign up" %>
    </div>
<% end %>

  